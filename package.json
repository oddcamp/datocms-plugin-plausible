{
  "name": "datocms-plugin-plausible-link",
  "version": "0.1.0",
  "private": true,
  "description": "Provides a sidebar link to plausible for the current record",
  "dependencies": {
    "@types/node": "^16.11.21",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "datocms-plugin-sdk": "^0.5.1",
    "datocms-react-ui": "^0.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "5.0.0",
    "typescript": "^4.5.5"
  },
  "scripts": {
    "start": "cross-env BROWSER='none' PUBLIC_URL='/' react-scripts start",
    "build": "cross-env PUBLIC_URL='.' react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "prepublishOnly": "npm run build"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "cross-env": "^7.0.3"
  },
  "keywords": [
    "datocms-plugin"
  ],
  "datoCmsPlugin": {
    "title": "Plausible Link",
    "coverImage": "docs/cover.png",
    "entryPoint": "build/index.html",
    "permissions": []
  }
}
